#zshrc functions included from .zshrc

function pushconf()
{
    cd ~/configfiles
    git commit -a
    git push
    cd $OLDPWD
    cd ~/sensitive
    git commit -a 
    git push
    cd $OLDPWD
}

function updateconf()
{
    cd ~/configfiles
    git pull origin master
    cd $OLDPWD
    cd ~/sensitive
    git pull origin master
    cd $OLDPWD
}

